{
    "name": "avisota/contao-css-to-inline-style",
    "description": "Avisota newsletter replace css to inline styles",
    "keywords": [
        "avisota",
        "email",
        "mail",
        "newsletter",
        "marketing",
        "css",
        "inline",
        "style"
    ],
    "type": "contao-module",
    "homepage": "http://avisota.org",
    "require": {
        "php": "^5.6",
        "contao/core": "^3.5.5",
        "contao-community-alliance/composer-plugin": "^2.4",
        "contao-community-alliance/dc-general": "^2.0@beta",
        "contao-community-alliance/events-create-options": "^1.0@dev",
        "avisota/contao-core": "^3.1",
        "tijsverkoyen/css-to-inline-styles": "^2.0"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2"
    },
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@avisota.org",
        "issues": "https://github.com/avisota/contao-css-to-inline-style/issues",
        "forum": "https://community.contao.org/de/forumdisplay.php?121-Avisota",
        "wiki": "http://de.contaowiki.org/Kategorie:Avisota",
        "irc": "irc://irc.freenode.org/avisota",
        "source": "https://github.com/avisota/contao-css-to-inline-style"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Avisota\\Contao\\Message\\Renderer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avisota\\Test\\Contao\\Message\\Renderer\\": "tests/Test/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/avisota-css-to-inline-style"
            }
        },
        "branch-alias": {
            "dev-develop": "3.2.x-dev"
        }
    }
}
