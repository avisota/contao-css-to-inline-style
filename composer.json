        "tijsverkoyen/css-to-inline-styles": "~1.0"
    },
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "baumann.sv@gmail.com",
        "issues": "https://github.com/avisota/contao-css-to-inline-style/issues",
        "forum": "https://community.contao.org/de/forumdisplay.php?121-Avisota",
        "wiki": "http://de.contaowiki.org/Kategorie:Avisota",
        "irc": "irc://irc.freenode.org/avisota",
        "source": "https://github.com/avisota/contao-css-to-inline-style"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
